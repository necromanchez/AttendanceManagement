                    </div><!-- /.box -->
                </div><!-- /.col -->
            </div><!-- /.row -->
            <div class="row">
                <div class="col-sm-6">
                    <div class="box box-primary">
                        <div class="box-header with-border">
                            <h3 class="box-title">DTR Form</h3>
                            <span class="label label-primary pull-right"><i class="fa fa-database"></i></span>
                        </div><!-- /.box-header -->
                        <div class="box-body">
                            <p>Template for Batch Upload of DTR.</p>
                            <a href="../../Correction/Templates/DownloadTemplate?filename=DTRForm.xlsx" class="btn btn-primary"><i class="fa fa-download"></i> Download</a>
                        </div><!-- /.box-body -->
                    </div><!-- /.box -->
                </div><!-- /.col -->
               
            </div><!-- /.row -->

        </div>

        <div class="box-body">
        </div>
    </div>
</section>



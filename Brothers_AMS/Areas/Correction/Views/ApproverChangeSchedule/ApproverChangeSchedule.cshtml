
@{
    ViewBag.Title = "ApproverChangeSchedule";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .resized {
        width: 146.813px !important;
    }
</style>
<link href="~/Content/assets/vendor_components/select2/dist/css/select2.min.css" rel="stylesheet" />
<section class="content-header">
    <h1>
        Change Schedule Approver
    </h1>
</section>
<section class="content">
    <div class="box box-primary">
        <div class="box-body pad">

            <form action="" method="POST" role="form">
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <div class="input-group">
                                <span class="input-group-addon resized">Section</span>
                                <select class="form-control autof select2" id="Section" name="Section"></select>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <div class="input-group">
                                <span class="input-group-addon resized">Date From</span>
                                <input type="text" class="form-control autof" id="DateFrom" name="DateFrom">
                            </div>

                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <div class="input-group">
                                <span class="input-group-addon resized">Date To</span>
                                <input type="text" class="form-control autof" id="DateTo" name="DateTo">
                            </div>

                        </div>
                    </div>
                </div>

              
            </form>

        </div>
        <div class="box-body">
            <table id="ChangeScheduleApprovertable" class="table table-bordered table-striped table-responsive">
                <thead>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</section>


<div class="modal fade AgencyEmail" tabindex="-1" role="dialog" aria-labelledby="bannerformmodal" aria-hidden="true" id="CSDetails">
    <div class="modal-dialog modal-lg">
     
        <div class="modal-content modalsizer">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h3 class="modal-title" id="myModalLabel">Employees</h3>
            </div>
            <div class="modal-body fixed-modal-height div-chk-less-height">

                <div class="table-responsive">
                    <div class="table-responsive">
                        <div class="table-responsive">
                            <table id="CSApproverDetails" class="table table-bordered table-striped table-responsive">
                                <thead>
                                    <tr>
                                        <th>
                                            <input type="checkbox" id="checkall_emp" class="filled-in chk-col-light-blue" />
                                            <label class="checker" for="checkall_emp"></label>
                                        </th>
                                        @*<th> CS Reference No </th>*@
                                        <th> Employee No </th>
                                        <th> Employee Name </th>
                                        <th> Section </th>
                                        <th> CSType </th>
                                        <th> Reason </th>
                                        <th> Date From </th>
                                        <th> Date To </th>
                                        <th> Shift </th>
                                        <th> Requestor </th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>

                        </div>

                    </div>

                </div>
                <div class="form-group">
                    <div class="input-group">
                        <div class="col-md-4">
                            <button type="button" class="btn btn-flat bg-green btn-lg btn-block theapprovebtn" id="btnApprovedRequest">
                                <i class="fa fa-upload"></i> Approve
                            </button>
                        </div>
                        <div class="col-md-4">
                            <button type="button" class="btn btn-flat bg-orange btn-lg btn-block theapprovebtn" id="btnRejectRequest">
                                <i class="fa fa-address-book"></i> Reject
                            </button>
                        </div>
                        <div class="col-md-4">
                            <button type="button" class="btn btn-flat bg-yellow btn-lg btn-block" id="btnCancel">
                                <i class="fa fa-address-trash"></i> Cancel
                            </button>
                        </div>
                    </div>
                    @*<label class="Noteme">NOTE: Application request can be approve tomorrow </label>*@

                </div>
            </div>
        </div>
      
    </div>
</div>





@section Scripts{
    <script src="~/Content/plugins/daterangepicker/moment.js"></script>
    <script src="~/Areas/Correction/Javascripts/ChangeScheduleApprover.js"></script>
}





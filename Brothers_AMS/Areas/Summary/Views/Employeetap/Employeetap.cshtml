
@{
    ViewBag.Title = "Employeetap";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content-header">
    <h1>Employee Tap</h1>


</section>
<section class="content">
    <div class="box box-default">
        <div class="box-header">
            <i class="fa fa-users"></i>
            <h3 class="box-title">Employee Tap List</h3>
        </div>
        <div class="box-body pad">
            <div class="row">
                <div class="col-md-2">
                    <div class="form-group">
                        <div class="input-group">
                            <span class="input-group-addon">Date Filter</span>
                            <input type="text" class="form-control step fil" id="DateFilter" name="DateFilter">
                        </div>

                    </div>
                </div>
            </div>
            <div class="table-responsive">
                <table class="table table-striped table-no-bordered table-hover" style="width:100%;" id="TapTable"></table>
            </div>
        </div>
    </div>
</section>

@section Scripts{
    <script src="~/Content/plugins/daterangepicker/moment.js"></script>

    <script src="~/Areas/Masters/Javascripts/Employeetap.js"></script>
}


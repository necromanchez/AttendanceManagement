
@{
    ViewBag.Title = "ErrorLogs";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content">
    <div class="box box-primary">
        <div class="box-header">
            <i class="fa fa-clock-o"></i>
            <h3 class="box-title">Error Logs</h3>
        </div>

        <div class="box-body">
            <div class="row">
                <div class="col-md-2">
                    <div class="form-group">
                        <div class="input-group">
                            <span class="input-group-addon">Date Filter</span>
                            <input type="text" class="form-control step fil" id="DateFilter" name="DateFilter">
                        </div>

                    </div>
                </div>
            </div>
            <table class="table table-striped table-no-bordered table-hover" id="ErrorLogTable">
                <thead>

                </thead>
            </table>
        </div>
    </div>
</section>

@section Scripts{
    <script src="~/Content/plugins/daterangepicker/moment.js"></script>
    <script src="~/Areas/Masters/Javascripts/ErrorLogs.js"></script>
}
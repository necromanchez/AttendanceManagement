@using Brothers_WMS.Models;
@using System.Data.SqlClient;
@using Brothers_WMS.Controllers;
@using System.Configuration;
@{
    string UserSection = string.Empty;
    if ((M_Users)(Session["user"]) != null)
    {
        M_Users UserInfo = (M_Users)(Session["user"]);
        UserSection = UserInfo.Section;
    }
    else
    {
        Url.Action("Logout", "Login");
    }
}
<section class="content-header">
    <h1>
        Cost Center
    </h1>
   
</section>
<section class="content">
    <div class="box box-primary">
        <div class="box-header">
            <i class="fa fa-money"></i>
            <h3 class="box-title">Cost Center Details</h3>
@if (@UserSection == null)
{
            <h3 class="box-title"><button id="Syncit" type="button" class="btn btn-block btn-primary btn-lg"><i class="fa fa-refresh"></i> Synchronize</button></h3>
}
        </div>
        <div class="box-body">
            <table id="CostCentertable" class="table table-bordered table-striped table-responsive">
                <thead>
                    
                </thead>
                <tbody>
                   
                </tbody>

            </table>
        </div>
    </div>
</section>


@section Scripts{
<script src="~/Areas/Masters/Javascripts/CostCenter.js"></script>
}





